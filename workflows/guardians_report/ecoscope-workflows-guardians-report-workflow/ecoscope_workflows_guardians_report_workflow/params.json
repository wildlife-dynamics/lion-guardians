    "groupers": {
      "additionalProperties": false,
      "properties": {
        "groupers": {
          "default": null,
          "items": {
            "anyOf": [
              {
                "$ref": "#/$defs/ValueGrouper"
              },
              {
                "$ref": "#/$defs/TemporalGrouper"
              }
